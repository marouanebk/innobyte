{
  "name": "facture-make",
  "version": "0.1.0",
  "private": true,
  "description": "ss",
  "author": {
    "name": "sof"
  },
  "main": "electron/main.js",
  "build": {
    "productName": "Gestion de facturation",
    "appId": "facture",
    "extends": null,
    "files": [
      "tailwind.config.json",
      "postcss.config.json",
      "package.json",
      "build/**/*",
      "node_modules/**/*",
      "electron/main.js",
      "electron/backend.js"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "uninstallDisplayName": "Unins",
      "oneClick": "false",
      "allowToChangeInstallationDirectory": "true"
    },
    "directories": {
      "output": "releases",
      "buildResources": "build"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.5.1",
    "concurrently": "^8.2.2",
    "convertir-nombre-lettre": "^1.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "express": "^4.18.2",
    "framer-motion": "^7.6.4",
    "fs": "^0.0.1-security",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.1",
    "jspdf": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.9.0",
    "swiper": "^7.3.1",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4"
  },
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env  npm start\" \"npm run electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on http://127.0.0.1:3000 && electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "electron": "^27.0.2",
    "electron-packager": "^17.1.2",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1"
  }
}
